{
  "name": "@wara/server-core",
  "version": "0.0.0",
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "db:push": "prisma db push --schema prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema prisma/schema.prisma",
    "db:generate": "prisma generate --schema prisma/schema.prisma",
    "db:studio": "prisma studio --schema prisma/schema.prisma"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.3.3",
    "@prisma/client": "^6.15.0",
    "better-auth": "^1.3.13",
    "dotenv": "^17.2.1",
    "elysia": "^1.3.21",
    "resend": "^6.0.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "bun-types": "^1.2.23",
    "prisma": "^6.15.0"
  }
}
